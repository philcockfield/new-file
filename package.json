{
  "name": "new-tmpl",
  "version": "1.0.0",
  "description": "Simple file templates.",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "bin": {
    "new": "./bin-cli"
  },
  "scripts": {
    "test": "sh sh/test.sh",
    "tdd": "yarn run test -- --reporter min --watch",
    "lint": "node node_modules/tslint/bin/tslint 'src/**/*.ts{,x}' --format verbose",
